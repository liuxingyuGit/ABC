<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ajax.dao.DistrictDao">

	<!-- 查询所有 -->
	<select id="getDistrictById" parameterType="int" resultType="district">
		SELECT *
		FROM district where cityId = #{value}
	</select>



</mapper>